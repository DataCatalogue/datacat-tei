   <define name="tei_place">
      <element name="place">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(place) contains data about a geographic location [13.3.4. Places]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="tei_model.headLike"/>
